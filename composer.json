{
    "name": "jordanbain/laravel-first-atlantic-commerce",
    "description": "First Atlantic Commerce EMV 3DS 2 payment processing driver for laravel",
    "type": "library",
    "require": {
        "php": "^8.0",
        "league/iso3166": "^4.0.0",
        "moneyphp/money": "^4.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/support": "^8.67|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jordanbain\\FirstAtlanticCommerce\\": "src/"
        }
    },
    "authors": [
        {
            "name": "jordan bain",
            "email": "jordanbaindev961@gmail.com"
        }
    ],
    "minimum-stability": "dev",

    "extra": {
        "laravel": {
            "providers": [
                "Jordanbain\\FirstAtlanticCommerce\\FirstAtlanticCommerceServiceProvider"
            ],
            "aliases": {
                "FirstAtlanticCommerce": "Jordanbain\\FirstAtlanticCommerce\\Facades\\FirstAtlanticCommerce"
            }
        }
    }
}
